package lesson2;

import java.util.Scanner;

public class Nenrei {

	public static void main(String[] args) {
				
		//Scannerクラスを初期化する
		Scanner Scan= new Scanner(System.in);
		
		System.out.println("年齢を入力してください。");
		//入力を受付ける部分
		int nenrei = Scan.nextInt();
				
		
		if (0==nenrei) {
			System.out.println("100円");
			
		}else if(0!=nenrei) {
			System.out.println("500円");
		}
		
	}

}
